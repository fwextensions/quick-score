{
    "plugins": ["plugins/markdown"],
	"source": {
		"include": "src"
	},
	"templates": {
		"default": {
			"layoutFile": "jsdoc/layout.tmpl",
			"staticFiles": {
				"include": [
					"jsdoc/static",
					"LICENSE"
				]
			}
		}
	},
	"opts": {
		"destination": "docs",
		"readme": "README.md"
	}
}
